{
  "name": "wats-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "postinstall": "node scripts/install-crypt-shim.js",
    "build:android:apk": "npx expo prebuild --platform android --clean && cd android && (if exist gradlew.bat (call gradlew.bat assembleRelease) else (./gradlew assembleRelease)) && cd ..",
    "build:android:aab": "npx expo prebuild --platform android --clean && cd android && (if exist gradlew.bat (call gradlew.bat bundleRelease) else (./gradlew bundleRelease)) && cd ..",
    "build:eas:apk": "eas build --platform android --profile preview --local",
    "build:eas:aab": "eas build --platform android --profile production --local",
    "build:web": "expo export --platform web",
    "build:web:local": "expo export --platform web --output-dir dist"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@supabase/supabase-js": "^2.39.0",
    "expo": "~54.0.0",
    "expo-av": "~15.0.1",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-file-system": "~19.0.21",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "flow-enums-runtime": "^0.0.6",
    "punycode": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@types/react": "~19.1.10",
    "charenc": "0.0.2",
    "is-buffer": "~1.1.6",
    "typescript": "~5.9.2",
    "whatwg-url": "^5.0.0"
  },
  "private": true,
  "engines": {
    "node": ">=20.19.0"
  }
}
